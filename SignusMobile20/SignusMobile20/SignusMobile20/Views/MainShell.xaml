<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:pages="clr-namespace:SignusMobile20;assembly=SignusMobile20"
             x:Class="SignusMobile20.MainShell">

    <Shell.Items>
        <ShellContent Title="Principal" Icon="">
            <pages:MainPage/>
        </ShellContent>
    </Shell.Items>

    <Shell.FlyoutHeader>
        <pages:HeaderView1/>
    </Shell.FlyoutHeader>

    <FlyoutItem Title="Bob" Icon="">
        <ShellContent ContentTemplate="{DataTemplate pages:pgTalhoesCR}"/>
    </FlyoutItem>

    <FlyoutItem Title="Rex" Icon="">
        <ShellContent ContentTemplate="{DataTemplate pages:RexPage1}"/>
    </FlyoutItem>

</Shell>